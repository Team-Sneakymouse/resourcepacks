{
    "multipart": [
        {   "apply": [{ "model": "block/birch_fence_post"},{ "model": "block/birch_fence_post", "y": 90 },{ "model": "block/birch_fence_post", "y": 180 },{ "model": "block/birch_fence_post", "y": 270 }]},
        {   "when": { "north": "true" },
            "apply": { "model": "block/birch_fence_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/birch_fence_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/birch_fence_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/birch_fence_side", "y": 270, "uvlock": true }
        }
    ]
}